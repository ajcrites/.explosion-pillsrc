[user]
	email = acrites@mobiquityinc.com
	name = Andrew Crites

[github]
	user = ajcrites

[heroku]
	user = ajcrites

[push]
	default = current

[pull]
	default = current

[hub]
	host = mobiquityinc.com

[color]
	ui = auto

[color "branch"]
	remote = red

[color "status"]
	added = green
	untracked = cyan
	removed = red

[color "diff"]
	whitespace = red reverse

[alias]
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
